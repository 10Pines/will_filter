<%= will_filter_tag(@events)%>

<%= will_filter_table_tag(@events, {
	:columns => [
		:id,
		[:creator_id, :value => lambda{|obj| obj.user ? obj.user.name : 'deleted user'}, :filterable => true, :title => 'Creator'],
		:headline,
		[:rank, {:style => "text-align:right; padding:0px;"}],
		:start_time
	]
}) %>
